#include<stdio.h>
int pass = 1;
void printArray(int a[],int n){
	for(int i=0;i<n;i++){
		printf("%d ",a[i]);
	}
	printf("\n");
}

void merge(int a[],int low,int mid,int high,int n){
	int i=low,j=mid+1,k=0;
	int temp[100];
	while(i<=mid && j<= high){
		if(a[i]<=a[j])
			temp[k++]=a[i++];
		else
			temp[k++]=a[j++];
	}
	while(i<=mid)
		temp[k++]=a[i++];
	while(j<=high)
		temp[k++]=a[j++];

	for(i=low,k=0;i<=high;i++,k++){
		a[i]=temp[k];
	}
	printf("Pass: ");
	for(int x = low;x<=high;x++){
		printf("%d ",a[x]);
	}
	printf("\n");
}

void mergeSort(int a[],int low,int high,int n){
	if(low<high){
		int mid = (low+high)/2;
}
